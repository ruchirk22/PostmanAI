{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/app.js"
    },
    {
      "src": "/main",
      "dest": "src/public/main.html"
    },
    {
      "src": "/(.*)",
      "dest": "src/public/index.html"
    }
  ]
}